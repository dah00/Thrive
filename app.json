{
  "expo": {
    "name": "Thrive-App",
    "slug": "Thrive-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "thriveapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Thrive.thriveapp",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleTypeRole": "Editor",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1014635338476-oqqtosvm84i14qfjm7dqdctr1dts9ldr"
            ]
          }
        ]
      },
      "excludeDevMenu": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./fake-path/google-services.json",
      "package": "com.Thrive.thriveapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosClientId": "1014635338476-oqqtosvm84i14qfjm7dqdctr1dts9ldr.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.1014635338476-oqqtosvm84i14qfjm7dqdctr1dts9ldr"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2dae2062-aa38-429d-bb4b-570e1f1115a9"
      },
      "devMenuEnabled": false
    }
  }
}
